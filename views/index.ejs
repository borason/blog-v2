<%- include('partials/header') -%>
<div class="row">
  <div class="col-2"></div>
  <div class="col-8">
    <h1>Home</h1>
    <h5><%=startingContent%></h5>
    <div class="row">
      <% posts.forEach((post) => {%>
      <div class="card bg-secondary mb-3" style="max-width: 20rem;">
        <div class="card-header">Header</div>
        <div class="card-body">
          <h4 class="card-title"><%=post.title%></h4>
          <p class="card-text"><%=post.content%></p>
        </div>
      </div>
      <%});%>
    </div>
  </div>
  <div class="col-2"></div>
</div>